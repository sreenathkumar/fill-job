{
   "compilerOptions": {
      "jsx": "react",
      "module": "CommonJS",
      "target": "es6",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "importHelpers": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
   },
   "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js",
   ],
   "exclude": [
      "node_modules",
   ],
}